<%= form_with scope: :event, url: events_path, local: true do |form| %>

	<p>
		<%= form.label :name %><br>
		<%= form.text_field :name %>
	</p>

	<p>
		<%= form.label :venue %><br>
		<%= form.text_field :venue %>
	</p>

	<p>
		<%= form.label :address %><br>
		<%= form.text_field :address %>
	</p>

	<p>
		<%= form.label :ticket_url %><br>
		<%= form.text_field :ticket_url %>
	</p>

	<p>
		<%= form.label :description %><br>
		<%= form.text_field :description %>
	</p>

	<p>
		<%= form.label :starttime %><br>
		<%= form.datetime_field :starttime %>
	</p>

	<p>
		<%= form.label :endtime %><br>
		<%= form.datetime_field :endtime %>
	</p>

	<p>
		<%= form.submit %>
	</p>
<% end %>